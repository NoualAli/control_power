{
    "name": "cretueusebiu/laravel-vue-spa",
    "description": "A Laravel-Vue SPA starter project template.",
    "keywords": [
        "spa",
        "laravel",
        "vue"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "barryvdh/laravel-dompdf": "^2.0",
        "beyondcode/laravel-websockets": "^1.14",
        "cerbero/laravel-enum": "^1.3",
        "fideloper/proxy": "^4.4",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.7",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^9.19",
        "laravel/tinker": "^2.6",
        "laravel/ui": "^3.3",
        "maatwebsite/excel": "^3.1",
        "opcodesio/log-viewer": "^3.1",
        "orangehill/iseed": "^3.0",
        "predis/predis": "^2.2",
        "staudenmeir/belongs-to-through": "^2.5",
        "staudenmeir/eloquent-has-many-deep": "^1.7",
        "tymon/jwt-auth": "dev-develop"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "doctrine/dbal": "^2.13",
        "fakerphp/faker": "^1.9.1",
        "laravel-lang/attributes": "^2.0",
        "laravel-lang/lang": "^12.0",
        "laravel-lang/publisher": "^14",
        "laravel/dusk": "^6.17",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.3.3",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "app/Helpers/Main.php",
            "app/Helpers/Data.php",
            "app/Helpers/Media.php",
            "app/Helpers/Response.php"
        ]
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php artisan jwt:secret --force --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
